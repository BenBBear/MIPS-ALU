`define NOT not #10
`define XOR xor #20

module bitSlice_1bit (Out, Cout, A, B,Cin,Cntrl);
  input A,B,Cin;
  input [2:0] Cntrl;
  output Out, Cout;
  
  wire Xor, Add, Sub, Slt, Mul, notB;
  
  add_1bit add(Add,Cout,A,B,Cin);
  `NOT notGate(notB,B);
  `XOR xorGate(Xor,A,B);
  
  mux_5bit finalMux(Out,Add,Sub,Xor,Slt,Mul,Cntrl[0],Cntrl[1],Cntrl[2]);
  
endmodule
